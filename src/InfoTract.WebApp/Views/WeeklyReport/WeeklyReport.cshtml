@{
    @model List<InfoTrack.Models.WebApp.Models.Ranking>
    ViewData["Title"] = "Weekly Report";
}

<style>
    table {
        width: 100%;
        border-collapse: collapse;
        margin-bottom: 20px;
    }

    table, th, td {
        border: 1px solid black;
        padding: 8px;
        text-align: left;
    }

    th {
        background-color: #f2f2f2;
    }
</style>
<h2>Website Ranking</h2>

<table>
    <thead>
        <tr>
            <th>Website Url</th>
            <th>Query</th>
            <th>Top Position</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Url</td>
                <td>@item.Keywords</td>
                <td>@item.Rank</td>
            </tr>
        }
    </tbody>
</table>
